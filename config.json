{
    "location": "/home/pi/mdesk/",
    "libraryRoot": "/home/pi/music/",
    "trace": {
        "tostdout": false,      // 日志过多输出至 stdout 竟然会卡死 sshd，只能重启设备
        "dumpsend": false,
        "dumprecv": false,
        "main": "debug",
        "audio": "debug",
        "worker": "noise"
    },
    "server": {
        "daemon": false,
        "port_contrl": 4001,    // tcp websocket socket
        "port_binary": 4002,    // ssl tcp websocket
        "broadcast_src": 4101,  // udp broadcast source port
        "broadcast_dst": 4102,  // udp broadcast destnation port
    }
}
